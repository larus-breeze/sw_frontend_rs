{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
      {
        "type": "cortex-debug",
        "request": "launch",
        "name": "Deb vario",
        "servertype": "openocd",
        "gdbPath": "/usr/bin/gdb-multiarch",
        "cwd": "${workspaceRoot}",
        "preLaunchTask": "Cargo Build (debug)",
        "runToEntryPoint": "main",
        "executable": "./target/thumbv7em-none-eabihf/debug/vario",
        "device": "STM32H743VGTx",
        "configFiles": [
          "interface/stlink.cfg",
          "target/stm32h7x_dual_bank.cfg"
        ],
        "postLaunchCommands": [
          "monitor rtt server start 8765 0",
          // should span the *right* part of RAM
          // get address $ rust-nm -S target/thumbv7*/debug/vario | grep RTT
          "monitor rtt setup 0x20000020 0x30 \"SEGGER RTT\"",
          "monitor rtt start",
      ],
        "svdFile": "${workspaceRoot}/STM32H743.svd",
      },
      {
        "type": "cortex-debug",
        "request": "launch",
        "name": "Run vario",
        "servertype": "openocd",
        "gdbPath": "/usr/bin/gdb-multiarch",
        "cwd": "${workspaceRoot}",
        "preLaunchTask": "Cargo Build (release)",
        "runToEntryPoint": "main",
        "executable": "./target/thumbv7em-none-eabihf/release/vario",
        "device": "STM32H743VGTx",
        "configFiles": [
          "interface/stlink.cfg",
          "target/stm32h7x_dual_bank.cfg"
        ],
        "postLaunchCommands": [
          "monitor rtt server start 8765 0",
          // should span the *right* part of RAM
          // get address $ rust-nm -S target/thumbv7*/release/vario | grep RTT
          "monitor rtt setup 0x20000020 0x30 \"SEGGER RTT\"",
          "monitor rtt start",
      ],
        "svdFile": "${workspaceRoot}/STM32H743.svd",
      },
      {
        "type": "cortex-debug",
        "request": "launch",
        "name": "Deb can",
        "servertype": "openocd",
        "gdbPath": "/usr/bin/gdb-multiarch",
        "cwd": "${workspaceRoot}",
        "preLaunchTask": "Cargo Build Examples (debug)",
        "runToEntryPoint": "main",
        "executable": "./target/thumbv7em-none-eabihf/debug/examples/can",
        "device": "STM32H743VGTx",
        "configFiles": [
          "interface/stlink.cfg",
          "target/stm32h7x_dual_bank.cfg"
        ],
        "postLaunchCommands": [
          "monitor rtt server start 8765 0",
          // should span the *right* part of RAM
          // get address $ rust-nm -S target/thumbv7*/debug/vario | grep RTT
          "monitor rtt setup 0x20000020 0x30 \"SEGGER RTT\"",
          "monitor rtt start",
      ],
        "svdFile": "${workspaceRoot}/STM32H743.svd",
      },
      {
        "type": "probe-rs-debug",
        "request": "launch",
        "name": "probe_rs example",
        "cwd": "${workspaceRoot}",
        "runtimeExecutable": "probe-rs",
        "runtimeArgs": ["dap-server"],
        "chip": "STM32H743VGTx",
        "flashingConfig": {
          "flashingEnabled": true,
          "haltAfterReset": false,
          "formatOptions": {
            "format": "elf"
          }
        },
        "coreConfigs": [
          {
            "coreIndex": 0,
            "programBinary": "./target/thumbv7em-none-eabihf/debug/examples/keys",
            "svdFile": "${workspaceRoot}/STM32H743.svd",
            "rttEnabled": true,
            "rttChannelFormats": [
              {
                "channelNumber": 0,
                // Format RTT data as String data
                "dataFormat": "String",
                // Include host-side timestamps for every line of data transferred from the target RTT output
                "showTimestamps": true
              },
              {
                "channelNumber": 1,
                // Treat data as raw binary data, and do not format in any way
                "dataFormat": "BinaryLE"
              },
            ]
          }
        ],
        "env": {
          //!MODIFY (or remove)
          // If you set this variable, check the VSCode console log window for the location of the log file.
          "RUST_LOG": "info"
        },
         // Info, Debug
        //"consoleLogLevel": "Console"
        "consoleLogLevel": "Console"
      }
    ]
  }

